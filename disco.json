{
    "version": "1.0",
    "services": {
        "web": {
            "port": 8080,
            "volumes": [
                {
                    "name": "images",
                    "destinationPath": "/images"
                },
                {
                    "name": "sqlite-data",
                    "destinationPath": "/sqlitedata"
                }
            ]
        }, "worker": {
            "command": "python worker.py",
            "volumes": [
                {
                    "name": "sqlite-data",
                    "destinationPath": "/sqlitedata"
                },
                {
                    "name": "images",
                    "destinationPath": "/images"
                }
            ]
        },
        "janitor": {
            "command": "python database_janitor.py",
            "volumes": [
                {
                    "name": "sqlite-data",
                    "destinationPath": "/sqlitedata"
                },
                {
                    "name": "images",
                    "destinationPath": "/images"
                }
            ]
        }
    }
}